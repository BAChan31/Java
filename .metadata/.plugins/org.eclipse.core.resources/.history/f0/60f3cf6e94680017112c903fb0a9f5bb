import java.util.ArrayList;
import java.util.HashMap;

public class HashTest {

	public class Student{
		private String no;
		private String name;
		private int age;
		
		public Student(String _no, String _name, int _age)
		{
			this.no = _no;
			this.name = _name;
			this.age = _age;
		}
		
		public String getNo(){
			return no;
		}
		
		public String getName(){
			return name;
		}
		
		public int getAge(){
			return age;
		}
	}
	
	public static void main(String args[])
	{
		//Create Student Data
		Student st1 = new HashTest().new Student("1번쨰", "강백호", 17);
	}
}
