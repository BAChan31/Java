package com.basicjava.interfaceEx2;

public class Toy implements Airplane, Teddybear, Mazingar {

	@Override
	public void getSpecM() {
		// TODO Auto-generated method stub
		System.out.println("마징가입니다");
		System.out.println("팔다리를 움직일 수 있습니다.");
		System.out.println("미사일을 발사할 수 있습니다.");
	}

	@Override
	public void getSpecT() {
		// TODO Auto-generated method stub
		System.out.println("곰돌이입니다");
		System.out.println("팔다리를 움직일 수 있습니다.");
		
	}

	@Override
	public void getSpecA() {
		// TODO Auto-generated method stub
		System.out.println("비행기입니다");
		System.out.println("불빛을 발사할 수 있습니다.");
		System.out.println("미사일을 발사할 수 있습니다.");
	}

	
}
